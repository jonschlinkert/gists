    "fork",
    "forks",
    "get",
    "gist",
    "gists",
    "github",
    "isstarred",
    "list",
    "paginate",
    "paginated",
    "patch",
    "post",
    "put",
    "request",
    "revision",
    "star",
    "starred",
    "unstar"
